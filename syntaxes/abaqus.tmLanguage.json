{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Abaqus",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#parametric"
		},
		{
			"include": "#parameterValue"
		},
		{
			"include": "#constants"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"comment": "Comment line",
					"begin": "^[*][*]",
					"end": "(?=\\n)",
					"name": "comment.line.abaqus"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"comment": "Numerical constants",
					"name": "string.abaqus",
					"match": "([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"comment": "Abaqus keywords",
					"name": "keyword.control.abaqus",
					"begin": "^[*]",
					"end": "(?=\\,|\\n)"
				}
			]
		},
		"parametric": {
			"patterns": [
				{
					"comment": "Parametric variables",
					"name": "entity.name.function.abaqus",
					"begin": "(?=<)",
					"end": "(>)"
				}
			]
		},
		"variables": {
			"patterns": [
				{
					"comment": "Variables",
					"name": "constant.language.abaqus",
					"match": "\\w+(?==)"
				}
			]
		},
		"parameterValue": {
			"patterns": [
				{
					"comment": "ParameterValue variables",
					"name": "variable.parameter.abaqus",
					"match": "(?<==)[^,*\\n]+(?<=)"
				}
			]
		},
		"parameterValue2": {
			"patterns": [
				{
					"comment": "ParameterValue2 variables",
					"name": "variable.parameter.abaqus",
					"match": "(?<=,)[^,=*0-9\n]+(?=,)"
				}
			]
		},
		"parameterValue3": {
			"patterns": [
				{
					"comment": "ParameterValue2 variables",
					"name": "variable.parameter.abaqus",
					"match": "(?<=[^0-9],)[^,=*0-9\n]+(?=\n)"
				}
			]
		}
	},
	"scopeName": "source.abaqus"
}