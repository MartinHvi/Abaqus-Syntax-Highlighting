{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Abaqus",
	"patterns": [
		{
			"include": "#headertext"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#parametric"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#variableValue"
		},
		{
			"include": "#otherVariableValue"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"comment": "Comment line",
					"begin": "^[*][*]",
					"end": "(?=\\n)",
					"name": "comment.line.abaqus"
				}
			]
		},
		"headertext": {
			"patterns": [
				{
					"comment": "Header text",
					"name": "entity.name.function.abaqus",
					"begin": "(?<=HEADING)",
					"end": "(?=\\*)"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"comment": "Numerical constants",
					"name": "string.abaqus",
					"match": "([+-]?(?=\\.\\d|\\d)(?:\\d+)?(?:\\.?\\d*))(?:[eE]([+-]?\\d+))?"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"comment": "Abaqus keywords",
					"name": "keyword.control.abaqus",
					"begin": "^[*]",
					"end": "(?=\\,|\\n)"
				}
			]
		},
		"parametric": {
			"patterns": [
				{
					"comment": "Parametric variables",
					"name": "entity.name.function.abaqus",
					"begin": "(?=<)",
					"end": "(>)"
				}
			]
		},
		"variables": {
			"patterns": [
				{
					"comment": "Variables",
					"name": "constant.language.abaqus",
					"match": "\\w+(?==)"
				}
			]
		},
		"variableValue": {
			"patterns": [
				{
					"comment": "Variable values",
					"name": "variable.parameter.abaqus",
					"match": "(?<==)[^,*\\n]+(?<=)"
				}
			]
		},
		"otherVariableValue": {
			"patterns": [
				{
					"comment": "Other variable values",
					"name": "variable.parameter.abaqus",
					"match": "(?<=|\\s,)[A-Za-z0-9-_]+(?=,|\\s)"
				}
			]
		}
	},
	"scopeName": "source.abaqus"
}